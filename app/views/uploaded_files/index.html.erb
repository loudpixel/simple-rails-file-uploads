<h1>Listing uploaded_files</h1>

<table>
  <tr>
    <th>File size</th>
    <th>File type</th>
  </tr>

<% @uploaded_files.each do |uploaded_file| %>
  <tr>
    <td><%=h uploaded_file.file_size %></td>
    <td><%=h uploaded_file.file_type %></td>
    <td><%= link_to 'Show', uploaded_file %></td>
    <td><%= link_to 'Edit', edit_uploaded_file_path(uploaded_file) %></td>
    <td><%= link_to 'Destroy', uploaded_file, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New uploaded_file', new_uploaded_file_path %>